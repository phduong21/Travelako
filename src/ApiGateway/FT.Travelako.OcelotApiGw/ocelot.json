  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5010",
    "RequestIdKey": "OcRequestId",
    "UseServiceDiscovery": true,
    "ServiceDiscoveryProvider": {
      "Host": "consul",
      "Port": 8500,
      "Type": "PollConsul",
      "PollingInterval": 100
    }
  }
